{
  "name": "lean-coffee-board",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:strapi",
    "start:server": "node server.js",
    "start:strapi": "cd strapi && npm start",
    "build": "npm-run-all --parallel build:client build:strapi && cp -r ./client/build/* ./strapi/public",
    "build:client": "cd client && npm run build",
    "build:strapi": "cd strapi && npm run build",
    "build:storybook": "cd client && npm run build-storybook",
    "dev": "npm-run-all --parallel server client strapi",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "strapi": "cd strapi && npm run develop",
    "test": "cd client && npm test",
    "storybook": "cd client && npm run storybook",
    "postinstall": "cd client && npm install && cd ../strapi && npm install"
  },
  "keywords": [],
  "author": "Jeremias Erbs <jerry@badabam.de> (https://badabam.de/)",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.11.18",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2"
  }
}

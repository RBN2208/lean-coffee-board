{
  "name": "strapi",
  "private": true,
  "version": "0.1.0",
  "description": "A Strapi application",
  "scripts": {
    "start": "strapi start",
    "build": "npm-run-all --parallel build:client build:strapi",
    "build:client": "cd client && npm run build",
    "build:strapi": "strapi build",
    "dev": "npm-run-all --parallel dev:strapi dev:client",
    "dev:client": "cd client && npm start",
    "dev:strapi": "strapi develop",
    "test": "cd client && npm test",
    "storybook": "cd client && npm run storybook",
    "postinstall": "cd client && npm install",
    "strapi": "strapi"
  },
  "devDependencies": {
    "npm-run-all": "4.1.5"
  },
  "dependencies": {
    "strapi": "3.5.3",
    "strapi-admin": "3.5.3",
    "strapi-utils": "3.5.3",
    "strapi-plugin-content-type-builder": "3.5.3",
    "strapi-plugin-content-manager": "3.5.3",
    "strapi-plugin-users-permissions": "3.5.3",
    "strapi-plugin-email": "3.5.3",
    "strapi-plugin-upload": "3.5.3",
    "strapi-connector-mongoose": "3.5.3"
  },
  "author": {
    "name": "Jerry Erbs"
  },
  "strapi": {
    "uuid": "0bb7f875-cc72-4df5-965e-f750200aea0e"
  },
  "engines": {
    "node": ">=10.16.0 <=14.x.x",
    "npm": "^6.0.0"
  },
  "license": "MIT"
}
